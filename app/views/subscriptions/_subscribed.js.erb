//debugger;
var subscribedBlock = document.getElementById("subscribed_block_<%= @room_id %>");
var subscribedCount = document.getElementById("subscribed_count");

subscribedBlock.innerHTML ="";
subscribedBlock.insertAdjacentHTML("beforeend", "<%= escape_javascript(render partial: 'subscriptions/subscription', room: @room) %>");

if(subscribedCount != null) {
	subscribedCount.innerHTML = "";
	subscribedCount.insertAdjacentHTML("beforeend", "<%= escape_javascript(@room.subscriptions.count.to_s) %>")
}
